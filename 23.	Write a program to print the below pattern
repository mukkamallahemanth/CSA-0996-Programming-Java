public class NumberPattern {
    public static void main(String[] args) {
        int n = 4; // Change n to the number of rows you want

        for (int i = 1; i <= n; i++) {
            // Print numbers from 1 to i
            for (int j = 1; j <= i; j++) {
                System.out.print(i + " ");
            }
            System.out.println();
        }
    }
}
